  <div class="row no-gutters align-items-center justify-content-center">
    <div class="col-md-4 py-4 text-center">
      <a href="#">
        <img width="140" src="<%= image_path 'logo_sm_dark.png' %>">
      </a>
    </div>
    <div class="col-md-4 p-4">
      <h5>Contact</h5>
      <ul>
        <li>
          <i class="fa fa-phone fa-2x"></i>
          <a href="tel:+40727.456.250">&nbsp;&nbsp;
            +40727.456.250
          </a>
        </li>
        <li>
          <i class="fa fa-envelope fa-2x"></i>
          <a href="mailto:office@go-park.ro">&nbsp;&nbsp;
            office@go-park.ro
          </a>
        </li>
      </ul>
      <hr/>
      <div class="text-center">
        <a class="social-lg" href="https://www.facebook.com/softspot.ro/" target="_blank">
          <span class="fa fa-facebook "></span>
        </a>
        <a class="social-lg" href="http://www.twitter.com/go-park" target="_blank">
          <span class="fa fa-instagram icon-md icon-carmine-pink"></span>
        </a>
        <a class="social-lg" href="https://www.facebook.com/softspot.ro/" target="_blank">
          <span class="fa fa-google-plus"></span>
        </a>
        <a class="social-lg" href="https://www.facebook.com/softspot.ro/" target="_blank">
          <span class="fa fa-twitter"></span>
        </a>
      </div>
    </div>
    <div class="col-md-4 p-3 contact-form">
      <form method="POST" action="http://formspree.io/office@go-park.ro">
        <div class="form-group">
          <input required type="email" name="email" class="form-control" placeholder="Email">
        </div>
        <div class="form-group">
          <textarea required class="form-control" name="message" placeholder="Spune-ne cum te putem ajuta..."></textarea>
