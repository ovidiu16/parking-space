        <button type="submit" class="btn btn-sm btn-danger">Trimite</button>
      </form>
    </div>
  </div>
</div>
<div class="ending row">
  <div class="col-12 col-md-8 col-md-offset-2 text-center mg-md">
    &copy; <a href="mailto:office@go-park.ro" target="_blank">Softspot</a> 2017.
    All rights reserved
  </div>
</div>
<!-- Footer - bloc-8 END -->


<!-- Google Analytics -->

